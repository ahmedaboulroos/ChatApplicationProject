<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.web.WebView?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="eg.gov.iti.jets.controllers.AnnouncementViewController">
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
    <center>
        <WebView fx:id="content" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <top>
        <HBox BorderPane.alignment="CENTER">
            <children>
                <Text fx:id="title" fill="#757575" strokeType="OUTSIDE" strokeWidth="0.0" text="Text"
                      HBox.hgrow="ALWAYS">
                    <font>
                        <Font name="Microsoft New Tai Lue" size="18.0"/>
                    </font>
                </Text>
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0"/>
            </BorderPane.margin>
        </HBox>
    </top>
</BorderPane>
