<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="rightContentPane" alignment="CENTER" prefWidth="900.0"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="eg.gov.iti.jets.views.DashboardViewController">
    <children>
        <VBox alignment="CENTER_RIGHT" prefHeight="142.0" prefWidth="900.0" VBox.vgrow="ALWAYS">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="143.0" prefWidth="900.0" VBox.vgrow="ALWAYS">
                    <children>
                        <ListView fx:id="listView" prefHeight="143.0" prefWidth="584.0">
                            <HBox.margin>
                                <Insets right="220.0"/>
                            </HBox.margin>
                        </ListView>
                        <ImageView fitHeight="102.0" fitWidth="96.0" pickOnBounds="true" preserveRatio="true"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <HBox style="-fx-background-color: #eee; -fx-border-color: #0099cc;">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="20.0"/>
            </padding>
            <children>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      prefHeight="250.0" style="-fx-background-color: white;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </HBox.margin>
                    <children>
                        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="323.0"
                              style="-fx-background-color: #0099cc;" VBox.vgrow="NEVER">
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                            </padding>
                            <children>
                                <Label alignment="CENTER" prefHeight="18.0" prefWidth="315.0"
                                       text="ON-Line/OFF-Line Users" textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                        <StackPane fx:id="userstatusChartPane" prefHeight="170.0" prefWidth="200.0">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </StackPane>
                    </children>
                </VBox>
                <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      prefHeight="250.0" style="-fx-background-color: white;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </HBox.margin>
                    <children>
                        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="323.0"
                              style="-fx-background-color: #0099cc;" VBox.vgrow="NEVER">
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                            </padding>
                            <children>
                                <Label alignment="CENTER" prefHeight="18.0" prefWidth="315.0" text=" Users  Gender"
                                       textFill="WHITE" HBox.hgrow="ALWAYS">
                                    <font>
                                        <Font name="System Bold" size="18.0"/>
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                        <StackPane fx:id="userGenderChartPane" prefHeight="170.0" prefWidth="200.0">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </StackPane>
                    </children>
                </VBox>

            </children>
            <VBox.margin>
                <Insets top="30.0"/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>
